# -----------------------------------------------------------------------------
# Timezone
# -----------------------------------------------------------------------------
TZ=Europe/Moscow

# -----------------------------------------------------------------------------
# Telegram Bot
# -----------------------------------------------------------------------------
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# -----------------------------------------------------------------------------
# Persistence Layer
# -----------------------------------------------------------------------------

# PostgreSQL Configuration
POSTGRES_HOST=ximanager-postgres
POSTGRES_PORT=5432
POSTGRES_USER=your_database_user_here
POSTGRES_PASSWORD=your_postgres_password_here
POSTGRES_DB=your_database_db_here
POSTGRES_SSLMODE=disable
POSTGRES_TIMEZONE=Europe/Moscow

# Redis Configuration
REDIS_HOST=ximanager-redis
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DB=0
REDIS_MAX_RETRIES=5
REDIS_DIAL_TIMEOUT=5s

# -----------------------------------------------------------------------------
# AI API Keys
# -----------------------------------------------------------------------------
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# -----------------------------------------------------------------------------
# Proxy (HTTP/SOCKS5)
# -----------------------------------------------------------------------------
PROXY_ADDRESS=your_proxy_address_here
PROXY_USER=your_proxy_user_here
PROXY_PASS=your_proxy_password_here

# -----------------------------------------------------------------------------
# Agent Prompts (base64 encoded)
# -----------------------------------------------------------------------------
AGENT_CONTEXT_SELECTION_PROMPT=your_agent_context_selection_prompt_here
AGENT_MODEL_SELECTION_PROMPT=your_agent_model_selection_prompt_here
AGENT_RESPONSE_LENGTH_PROMPT=your_agent_response_length_prompt_here
SUMMARIZATION_PROMPT=your_summarization_prompt_here
AGENT_PERSONALIZATION_VALIDATION_PROMPT=your_agent_personalization_validation_prompt_here
AGENT_PERSONALIZATION_EXTRACTION_PROMPT=your_agent_personalization_extraction_prompt_here
AGENT_WEB_SEARCH_PROMPT=your_agent_web_search_prompt_here

# -----------------------------------------------------------------------------
# Unleash Service Environment Variables
# -----------------------------------------------------------------------------
UNLEASH_ADMIN_TOKEN=your_unleash_admin_token_here